<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="AccentColorBrush" Color="#CBB994"/>

    <LinearGradientBrush x:Key="SidebarGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#D8C6A7" Offset="0"/>
        <GradientStop Color="#CBB994" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="SidebarTextBrush" Color="#3E352A"/>
    <SolidColorBrush x:Key="SidebarDividerBrush" Color="#A6957A"/>
    <SolidColorBrush x:Key="ButtonHoverBrush" Color="#B8A988"/>

</ResourceDictionary>
